{
  "name": "panda-meet",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "ios": "expo run:ios",
    "android": "expo run:android",
    "dev": "expo start --dev-client",
    "dev:prod": "expo start --no-dev --minify",
    "prebuild": "expo prebuild",
    "build:ios:preview": "eas build --platform ios --profile preview --local",
    "build:android:preview": "eas build --platform android --profile preview --local",
    "build:ios": "eas build --platform ios --profile production --local",
    "build:android": "eas build --platform android --profile production --local",
    "clean": "git clean -xdf .expo node_modules ios android"
  },
  "dependencies": {
    "@pi4/ui": "workspace:*",
    "app": "workspace:*",
    "babel-plugin-module-resolver": "^5.0.2",
    "burnt": "^0.13.0",
    "expo": "~52.0.42",
    "expo-apple-authentication": "~7.1.3",
    "expo-build-properties": "~0.13.2",
    "expo-constants": "~17.0.8",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.19",
    "expo-font": "~13.0.4",
    "expo-image": "~2.0.7",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "7.0.5",
    "expo-router": "~4.0.20",
    "expo-splash-screen": "^0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-mmkv": "^2.12.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@expo/metro-config": "^0.19.12",
    "@tamagui/babel-plugin": "1.125.26",
    "metro-minify-terser": "^0.82.0",
    "typescript": "^5.8.3",
    "@expo/next-adapter": "^6.0.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
